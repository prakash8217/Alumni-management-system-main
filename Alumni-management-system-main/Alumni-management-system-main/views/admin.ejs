<h1 style="text-align: center; margin-top: 30px; margin-bottom: 30px;">Admin page</h1>

<div class="container" style="max-width: 600px;margin:0 auto;padding:50px;box-shadow: rgba(100, 100, 111, 0.2) 0px 7px 29px 0px;">
    <form action="/login" method="post">
        <div class="form-group" style="margin-bottom:30px;">
            <input type="text" placeholder="Enter Email:" name="username" class="form-control">
        </div>
        <div class="form-group" style="margin-bottom:30px;">
            <input type="password" placeholder="Enter Password:" name="password" class="form-control">
        </div>
        <div class="form-btn" style="margin-bottom:30px;">
            <input type="submit" value="Login" name="login" class="btn btn-primary">
        </div>
      </form>
</div>


